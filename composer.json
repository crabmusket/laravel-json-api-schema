{
    "name": "laravel-json-api/schema",
    "description": "Schemas describing JSON API resources.",
    "keywords": [
        "jsonapi.org",
        "json-api",
        "jsonapi",
        "json:api",
        "laravel"
    ],
    "homepage": "https://github.com/laravel-json-api/schema",
    "support": {
        "issues": "https://github.com/laravel-json-api/schema/issues"
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Cloud Creativity Ltd",
            "email": "info@cloudcreativity.co.uk"
        },
        {
            "name": "Christopher Gammie",
            "email": "contact@gammie.co.uk"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelJsonApi\\{NAME}\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelJsonApi\\{NAME}\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev",
            "dev-develop": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
